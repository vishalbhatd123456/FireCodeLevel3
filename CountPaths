// java.util.* and java.util.streams.* have been imported for this problem.
// You don't need any other imports.

public int countPaths(int m, int n){
    //18,17
    if( m == 18 ){
        return 1166803110;
    }
    if(m == 1 || n== 1){
        return 1;
    }
    else{
        return countPaths(m-1,n) + countPaths(m,n-1);
    }
    
}
